{
  "name": "platform",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "all:start": "npm-run-all --parallel start:*",
    "start:main": "npm run theme && node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.test.json --aot --port 4201",
    "start:react": "cd micro_apps/react && yarn start",
    "react": "cd micro_apps/react && yarn start:alone",
    "build": "npm run theme && node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod",
    "build:schematics": "./node_modules/.bin/tsc -p tsconfig.schematics.json",
    "copy:schemas": "cp --parents schematics/*/schema.json ./dist/platform/",
    "copy:files": "cp --parents -p schematics/*/files/** ./dist/platform/",
    "copy:collection": "cp schematics/collection.json ./dist/platform/schematics/collection.json",
    "postbuild": "npm run copy:schemas && npm run copy:files && npm run copy:collection",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:style",
    "e2e": "ng e2e",
    "analyze": "npm run color-less && node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "node scripts/color-less.js",
    "theme": "node scripts/generate-less-vars.js && node scripts/theme.js",
    "icon": "ng g ng-alain:plugin icon",
    "lint:ts": "ng lint --fix",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "hmr": "ng serve -c=hmr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.11",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/forms": "~9.1.11",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "@delon/abc": "^9.5.1",
    "@delon/acl": "^9.5.1",
    "@delon/auth": "^9.5.1",
    "@delon/cache": "^9.5.1",
    "@delon/chart": "^9.5.1",
    "@delon/form": "^9.5.1",
    "@delon/mock": "^9.5.1",
    "@delon/theme": "^9.5.1",
    "@delon/util": "^9.5.1",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^5.0.0",
    "ajv": "^6.12.2",
    "crypto-js": "^3.1.9-1",
    "ng-alain": "^9.5.1",
    "ng-zorro-antd": "^9.2.2",
    "ngx-markdown": "^10.0.0",
    "qiankun": "^2.0.23",
    "rxjs": "~6.5.4",
    "screenfull": "^5.0.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "9.1.0",
    "@angular-devkit/build-angular": "~0.901.10",
    "@angular/cli": "~9.1.10",
    "@angular/compiler-cli": "~9.1.11",
    "@angularclass/hmr": "^2.1.3",
    "@delon/testing": "^9.5.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "antd-theme-generator": "1.2.2",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "less": "^3.12.2",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-npm-import": "^2.1.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.2.4",
    "ng-alain": "^9.5.1",
    "ng-alain-codelyzer": "^0.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "stylelint": "^13.3.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "~3.8.3"
  },
  "schematics": "./schematics/collection.json",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  }
}
